{
  "name": "@corux/country-data",
  "version": "2.0.3",
  "description": "Data about countries and regions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate-data": "ts-node bin/generate-data.ts --destination src/data/",
    "pretest": "npm-run-all --parallel build lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "posttest": "cat ./coverage/lcov.info | codacy-coverage",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc && cp -a ./src/data dist/"
  },
  "author": "corux",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/corux/country-data.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.12",
    "@types/jest": "24.0.16",
    "axios": "0.19.0",
    "cheerio": "1.0.0-rc.3",
    "codacy-coverage": "3.4.0",
    "commander": "3.0.0",
    "countryjs": "1.8.0",
    "jest": "24.8.0",
    "json-stable-stringify": "1.0.1",
    "npm-run-all": "4.1.5",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
